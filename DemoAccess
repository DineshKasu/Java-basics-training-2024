package com.mycompany.day1project;


/**
 *
 * @author dinesh
 */
public class DemoAccess {
    
    

    //public -- avaialble from anywhere
    
     public String carName;

    //private -- avaialble only within class
     private String carModel;
    
    //default available within the same package
     
     String carManufactYear;
    
    //protected available within the same package and child class (inheritance parent/child)
     protected String carColor;
     
     
     public void getCarName(){
         
         System.out.println("Tesla Car");
     }
     
     private void getCarModel(){
         
         System.out.println("Tesla Y");
     }
     
     protected void getCarColor(){
         
         System.out.println("Tesla Blue");
     }
     
     void getCarManufactYear(){
         
         System.out.println("Tesla Y 2023");
     }
    
}
